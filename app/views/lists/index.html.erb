
  <div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url('data:image/jpeg;base64,');">
      <h1> Any kind of movies</h1>
  </div><br><br>

<div class="row">
  <div class="cards">
    <% @lists.each do |list| %>
      <div class='mt-2 card-category'>
        <a href='<%= list_path(list) %>'>
          <button class="styled rounded" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= list.image_url %>)"
            type="button">
            <%= list.name %>
          </button>
        </a>
      </div>
    <% end %>
  </div>
</div>
